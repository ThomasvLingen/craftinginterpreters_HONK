// Print statements
print(1 + 2 + 3);
print("hello" + " " + "HONK!");
print(true);

// Expression statements
3 * 4;
3 > 2;
3 != 1;

// Variables + expressions
var three = 3;
var five = 5;
print(three + five);
print(three < five);

// Scopes + shadowing
var a = "global a";
var b = "global b";
var c = "global c";
{
    var a = "outer a";
    var b = "outer b";
    {
        var a = "inner a";
        print(a);
        print(b);
        print(c);
    }
    print(a);
    print(b);
    print(c);
}
print(a);
print(b);
print(c);